{
  "name": "productcatalog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run tsc",
    "start": "yarn run build && node ./dist/server.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "lint": "yarn run eslint . --ext=.ts",
    "test": "yarn run jest",
    "migration:run": "yarn run ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "yarn run ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "yarn run ts-node ./node_modules/typeorm/cli.js migration:generate"
  },
  "devDependencies": {
    "@sendgrid/helpers": "^7.0.1",
    "@types/faker": "^4.1.11",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "prettier": "2.0.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.0.1",
    "apollo-server-koa": "^2.12.0",
    "graphql": "^15.0.0",
    "graphql-relay": "^0.6.0",
    "koa": "^2.11.0",
    "koa-helmet": "^5.2.0",
    "koa-router": "^8.0.8",
    "pg": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16",
    "typeorm": "^0.2.24"
  }
}
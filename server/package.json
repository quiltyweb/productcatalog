{
  "name": "productcatalog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run tsc",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "lint": "yarn run eslint . --ext=.ts",
    "test:integration": "yarn run jest ./test/integration",
    "test:unit": "yarn run jest ./test/unit",
    "typecheck": "yarn run tsc",
    "migration:run": "yarn run typeorm-ts-node-esm migration:run",
    "migration:revert": "yarn run typeorm-ts-node-esm migration:revert",
    "migration:generate": "yarn run typeorm migration:generate",
    "migration:create": "yarn run typeorm migration:create",
    "generate": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.5",
    "@types/faker": "^5.1.4",
    "@types/graphql-relay": "^0.7.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.4",
    "@types/mailgun-js": "^0.22.13",
    "@types/node": "^18.11.15",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.1.0",
    "jest": "^29.3.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "@adminjs/design-system": "^3.1.5",
    "@adminjs/express": "^5.0.1",
    "@adminjs/typeorm": "^4.0.0",
    "@adminjs/upload": "^3.0.1",
    "@apollo/server": "^4.0.5",
    "adminjs": "^6.6.5",
    "aws-sdk": "^2.917.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "graphql": "^16.2.0",
    "graphql-relay": "^0.10.0",
    "helmet": "^6.0.0",
    "mailgun-js": "^0.22.0",
    "pg": "^8.8.0",
    "react-player": "^2.11.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1",
    "typeorm": "^0.3.10"
  },
  "resolutions": {
    "@tiptap/core": "^2.0.0-beta.1",
    "@types/react": "^18.0.25",
    "prosemirror-view": "^1.29.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
